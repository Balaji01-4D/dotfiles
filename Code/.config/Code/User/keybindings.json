[
    {
        "key": "alt+a",
        "command": "workbench.action.chat.openAgent",
        "when": "!chatSetupHidden"
    },
    {
        "key": "ctrl+p",
        "command": "workbench.action.quickOpen",
        "when": "vim.mode == 'Normal' || vim.mode == 'Visual' || vim.mode == 'VisualLine' || vim.mode == 'VisualBlock'"
    },
    {
        "key": "ctrl+alt+v",
        "command": "toggleVim"
    },
    {
        "key": "ctrl+f",
        "command": "actions.find",
        "when": "vim.mode == 'Normal' || vim.mode == 'Visual' || vim.mode == 'VisualLine' || vim.mode == 'VisualBlock'"
    }
]
